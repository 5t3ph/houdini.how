{
    "workletName": "Scalloped Border",
    "packageName": "extra-underline",
    "workletUrl": "https://raw.githubusercontent.com/una/extra.css/master/lib/superUnderline/worklet.js",
    "cdnUrl": "https://unpkg.com/extra-scalloped-border@1.1.0/worklet.js",
    "npmUrl": "https://www.npmjs.com/package/extra-scalloped-border",
    "demoUrl": "https://codepen.io/una/pen/MRxWgN",
    "tags": ["paint"],
    "customProps": {
      "--extra-scallopColor": {
        "type": "color",
        "default": "#00eeff"
      },
      "--extra-scallopRadius": {
        "type": "number",
        "default": "5",
        "range": [0, 20]
      },
      "--extra-scallopWeight": {
        "type": "number",
        "default": "2",
        "range": [0, 20]
      }
    },
    "html": "<h2 style='padding: 1rem 1.5rem;'>Scalloped</h2>",
    "usage": "background: paint(extra-scallopedBorder);",
    "author": {
      "name": "@Una",
      "url": "https://twitter.com/una",
      "image": "https://webdev.imgix.net/images/authors/una@2x.jpg"
    }
  }