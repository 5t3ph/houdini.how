{
    "workletName": "Sparkles",
    "packageName": "extra-sparkles",
    "npmUrl": "",
    "workletUrl": "https://raw.githubusercontent.com/una/extra.css/master/lib/sparkles/worklet.js",
    "cdnUrl": "https://unpkg.com/extra.css@1.2.0/sparkles.js",
    "demoUrl": "https://glitch.com/~sparkles",
    "tags": ["paint", "properties and values"],
    "customProps": {
      "--extra-sparkleNumber" : {
        "type": "number",
        "default": "30",
        "range": [0, 500]
      },
      "--extra-sparkleHue" : {
        "type": "number",
        "default": "200",
        "range": [0, 360]
      },
      "--extra-sparkleHeightVariance": {
        "type": "number",
        "default": "9",
        "range": [0, 20]
      },
      "--extra-sparkleWidthVariance": {
        "type": "number",
        "default": "12",
        "range": [0, 20]
      },
      "--extra-sparkleWeightVariance": {
        "type": "number",
        "default": "2",
        "range": [1, 12]
      }
    },
    "html": "",
    "usage": "background: paint(extra-sparkles)",
    "author": {
      "name": "@Una",
      "url": "https://twitter.com/una",
      "image": "https://webdev.imgix.net/images/authors/una@2x.jpg"
    }
  }