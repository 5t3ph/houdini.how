{
  "workletName": "Sparkles",
  "packageName": "extra-sparkles",
  "cdnUrl": "https://unpkg.com/extra-sparkles/worklet.js",
  "npmUrl": "https://www.npmjs.com/package/extra-sparkles",
  "demoUrl": "https://codepen.io/una/pen/MRxgNN",
  "tags": ["paint"],
  "customProps": {
    "--extra-sparkleNumber": {
      "type": "number",
      "default": "30",
      "range": [0, 500]
    },
    "--extra-sparkleHue": {
      "type": "number",
      "default": "200",
      "range": [0, 360]
    },
    "--extra-sparkleHeightVariance": {
      "type": "number",
      "default": "9",
      "range": [0, 20]
    },
    "--extra-sparkleWidthVariance": {
      "type": "number",
      "default": "12",
      "range": [0, 20]
    },
    "--extra-sparkleWeightVariance": {
      "type": "number",
      "default": "2",
      "range": [1, 12]
    }
  },
  "html": "",
  "usage": "background: paint(extra-sparkles)",
  "author": {
    "name": "@Una",
    "url": "https://twitter.com/una",
    "image": "/assets/images/una.jpg"
  }
}
