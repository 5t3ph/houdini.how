{
  "workletName": "Bytemare",
  "packageName": "bytemare",
  "workletUrl": "https://raw.githubusercontent.com/malchata/bytemare/main/bytemare.js",
  "cdnUrl": "https://unpkg.com/bytemare/bytemare.js",
  "npmUrl": "https://www.npmjs.com/package/bytemare",
  "demoUrl": "http://paintlets.herokuapp.com/",
  "tags": ["paint"],
  "customProps": {
    "--bytemare-tile-size": {
      "type": "number",
      "default": "25",
      "range": [5, 100]
    },
    "--bytemare-color": {
      "type": "color",
      "default": "#23d7aa"
    },
    "--bytemare-gap": {
      "type": "number",
      "default": "1",
      "range": [0, 20]
    },
    "--bytemare-probability": {
      "type": "number",
      "default": "0.4",
      "range": [0, 0.95],
      "step": "0.05"
    }
  },
  "html": "",
  "usage": "background-image: paint(bytemare);",
  "author": {
    "name": "@malchata",
    "url": "https://twitter.com/malchata",
    "image": "/assets/images/malchata.jpg"
  }
}
