{
  "workletName": "border-radius-reverse",
  "packageName": "@houdini-modules/border-radius-reverse",
  "npmUrl": "",
  "cdnUrl": "https://unpkg.com/@houdini-modules/border-radius-reverse@0.0.1/border-radius-reverse.js",
  "demoUrl": "https://googlechromelabs.github.io/houdini-samples/paint-worklet/border-radius-reverse/",
  "tags": ["paint"],
  "customProps": {
    "--border-radius-reverse": {
      "type": "number",
      "default": "10",
      "range": [0, 50]
    },
    "--border-radius-reverse-color": {
      "type": "color",
      "default": "#ffc14c"
    }
  },
  "html": "<h2 contenteditable style='outline: none; padding: 1rem; font-size: 1.25rem;'>Content!</h2>",
  "usage": {
    "border": "calc(var(--border-radius-reverse) * 1px) solid transparent",
    "border-image": "paint(border-radius-reverse)",
    "border-image-slice": "var(--border-radius-reverse)"
  },
  "author": {
    "name": "Surma",
    "url": "https://twitter.com/dassurma",
    "image": "/assets/images/DasSurma.jpg"
  }
}
