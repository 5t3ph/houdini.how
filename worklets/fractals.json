{
  "workletName": "Fractals",
  "packageName": "css-houdini-fractals",
  "npmUrl": "https://www.npmjs.com/package/css-houdini-fractals",
  "workletUrl": "https://unpkg.com/css-houdini-fractals@1.0.0/fractals.js",
  "cdnUrl": "https://unpkg.com/css-houdini-fractals@1.0.0/fractals.js",
  "demoUrl": "https://conradsollitt.github.io/css-houdini-fractals/",
  "tags": ["paint"],
  "customProps": {
    "--colors": {
      "type": "string",
      "default": "red green blue cyan magenta yellow",
      "options": [
        "red green blue cyan magenta yellow",
        "red green blue",
        "black",
        "#000 #222 #444 #666 #888 #aaa #ccc"
      ]
    },
    "--angle": {
      "type": "number",
      "default": "30",
      "range": [0, 360],
      "step": "1"
    },
    "--starting-length-percent": {
      "type": "number",
      "default": "22",
      "range": [5, 95],
      "step": "1"
    },
    "--next-line-size": {
      "type": "number",
      "default": "0.8",
      "range": [0.1, 0.9],
      "step": "0.1"
    },
    "--max-draw-count": {
      "type": "number",
      "default": "100000",
      "range": [0, 2000000],
      "step": "1"
    },
    "--debug-to-console": {
      "type": "number",
      "default": "0",
      "range": [0, 1],
      "step": "1"
    }
  },
  "usage": "background-image: paint(fractals)",
  "author": {
    "name": "Conrad Sollitt",
    "url": "https://conradsollitt.com",
    "image": "/assets/images/conrad_sollitt.jpg"
  }
}
