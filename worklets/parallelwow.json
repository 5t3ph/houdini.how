{
  "workletName": "Parallel-o-wow",
  "packageName": "parallelowow",
  "cdnUrl": "https://unpkg.com/parallelowow/parallelowow.js",
  "npmUrl": "https://www.npmjs.com/package/parallelowow",
  "demoUrl": "http://paintlets.herokuapp.com/",
  "tags": ["paint"],
  "customProps": {
    "--parallelowow-tile-width": {
      "type": "number",
      "default": "40",
      "range": [10, 100]
    },
    "--parallelowow-base-color": {
      "type": "color",
      "default": "#ff5566"
    },
    "--parallelowow-color-step": {
      "type": "number",
      "default": "7",
      "range": [-10, 10]
    },
    "--parallelowow-probability": {
      "type": "number",
      "default": "0.6",
      "range": [0, 0.95],
      "step": "0.05"
    },
    "--parallelowow-stroke-weight": {
      "type": "number",
      "default": "0.5",
      "range": [0, 10]
    }
  },
  "html": "",
  "usage": "background: paint(parallelowow), var(--parallelowow-base-color);",
  "author": {
    "name": "@malchata",
    "url": "https://twitter.com/malchata",
    "image": "/assets/images/malchata.jpg"
  }
}
