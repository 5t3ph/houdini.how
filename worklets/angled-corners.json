{
  "workletName": "Angled Corners",
  "packageName": "angled-corners",
  "npmUrl": "https://www.npmjs.com/package/angled-corners",
  "cdnUrl": "https://unpkg.com/angled-corners",
  "demoUrl": "https://glitch.com/~angled-corners",
  "tags": ["paint"],
  "note": "Note: Partial demo for compatibility. For full effect using arguments, view <a href='https://glitch.com/~angled-corners'>Demo</a> with Experimental Web Platform Features flag on.",
  "customProps": {
    "--corner-radius": {
      "type": "string",
      "default": "0 20 0 0"
    },
    "--paint-color": {
      "type": "color",
      "default": "#6200ee"
    },
    "--stroke-weight": {
      "type": "number",
      "default": "4",
      "range": [0, 15]
    }
  },
  "usage": {
    "background": "paint(angled-corners)"
  },
  "html": "<button contenteditable style='outline: none; border: none; padding: 1rem; font-size: 1.25rem;'>Button</button>",
  "author": {
    "name": "@Una",
    "url": "https://twitter.com/una",
    "image": "/assets/images/una.jpg"
  }
}
