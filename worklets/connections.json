{
  "workletName": "Connections",
  "packageName": "css-houdini-connections",
  "npmUrl": "https://www.npmjs.com/package/css-houdini-connections",
  "workletUrl": "https://unpkg.com/css-houdini-connections/dist/connections.js",
  "cdnUrl": "https://unpkg.com/css-houdini-connections/dist/connections.js",
  "demoUrl": "https://codepen.io/nucliweb/pen/yLJdroy",
  "tags": ["paint"],
  "customProps": {
    "--connections-particleColor": {
      "type": "color",
      "default": "#142c92"
    },
    "--connections-lineColor": {
      "type": "color",
      "default": "#297bff"
    },
    "--connections-particleAmount": {
      "type": "number",
      "default": "140",
      "range": [80, 400],
      "step": "10"
    },
    "--connections-defaultRadius": {
      "type": "number",
      "default": 2,
      "range": [1, 5],
      "step": "1"
    },
    "--connections-variantRadius": {
      "type": "number",
      "default": 3,
      "range": [1, 5],
      "step": "1"
    },
    "--connections-linkRadius": {
      "type": "number",
      "default": 60,
      "range": [0, 120]
    }
  },
  "usage": "background: paint(connections)",
  "author": {
    "name": "@nucliweb",
    "url": "https://twitter.com/nucliweb",
    "image": "/assets/images/nucliweb.jpg"
  }
}
